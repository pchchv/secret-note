{
  "name": "bot",
  "version": "0.0.0",
  "description": "Telegrm bot for one-time notes service",
  "main": "bot.js",
  "repository": "git@github.com:pchchv/secret-note-rest-api.git",
  "author": "Jack <ipchchv@gmail.com>",
  "license": "MIT",
"scripts": {
  "distribute": "yarn build-ts && node dist/server.js",
  "develop": "tsc-watch --skipLibCheck --onSuccess 'node dist/server.js'",
  "build-ts": "tsc --skipLibCheck",
  "test": "yarn jest --forceExit --runInBand",
  "pretty": "prettier --check src",
  "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src"
},
"devDependencies": {
  "@types/jest": "^27.0.3",
  "@types/node": "^16.11.11",
  "@types/supertest": "^2.0.11",
  "@types/validator": "^13.7.2",
  "eslint": "^8.3.0",
  "eslint-config-prettier": "^8.3.0",
  "eslint-plugin-import": "^2.25.3",
  "eslint-plugin-no-relative-import-paths": "^1.0.8",
  "eslint-plugin-node": "^11.1.0",
  "eslint-plugin-prettier": "^4.0.0",
  "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
  "jest": "^27.4.0",
  "module-alias": "^2.2.2",
  "prettier": "^2.5.0",
  "source-map-support": "^0.5.21",
  "supertest": "^6.1.6",
  "ts-jest": "^27.0.7",
  "tsc-watch": "^4.5.0",
  "typescript": "^4.5.2"
},
"_moduleAliases": {
  "@": "dist"
},
"packageManager": "yarn@3.1.0",
"dependencies": {
  "dotenv": "^10.0.0",
  "tsc": "^2.0.4"
}
}